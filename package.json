{
  "name": "neurapath-app",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "license": "MIT",
  "packageManager": "bun@1.3",
  "engines": {
    "node": ">=22"
  },
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "cf-typegen": "wrangler types ./src/worker-configuration.d.ts",
    "deploy": "bun run build && bun run cf-typegen && wrangler deploy",
    "clean": "rm -rf .svelte-kit && rm -rf node_modules && rm -rf .wrangler && rm -rf .svelte-kit && rm -f bun.lock && rm -f package-lock.json && bun i"
  },
  "devDependencies": {
    "@internationalized/date": "^3.10.0",
    "@lucide/svelte": "^0.555.0",
    "@sveltejs/adapter-auto": "^7.0.0",
    "@sveltejs/adapter-cloudflare": "^7.2.4",
    "@sveltejs/kit": "^2.49.0",
    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.19",
    "@tailwindcss/vite": "^4.1.17",
    "@types/pdfjs-dist": "^2.10.378",
    "clsx": "^2.1.1",
    "layerchart": "^2.0.0-next.43",
    "mode-watcher": "^1.1.0",
    "svelte": "^5.45.2",
    "svelte-check": "^4.3.4",
    "svelte-sonner": "^1.0.6",
    "tailwind-merge": "^3.4.0",
    "tailwind-variants": "^3.2.2",
    "tailwindcss": "^4.1.17",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.9.3",
    "vite": "^7.2.4"
  },
  "dependencies": {
    "@svelte-put/cloudflare-turnstile": "^1.2.0",
    "@tsndr/cloudflare-worker-jwt": "^3.2.0",
    "bcrypt-ts": "^7.1.0",
    "bits-ui": "^2.14.4",
    "pdfjs-dist": "^5.4.449",
    "posthog-js": "^1.298.1",
    "posthog-node": "^5.14.1",
    "quill": "^2.0.3"
  }
}